{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      }
    ],
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ]
  }
  